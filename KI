    local chests = {
        "Golden Chest","Enchanted Chest","Mythical Chest",
        "Magma Chest","Legends Chest","Jungle Chest"
    }
    for _, c in ipairs(chests) do
        chestEvent:InvokeServer(c)
        task.wait(0.1)
    end
end)

